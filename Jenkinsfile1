pipeline {
    agent any

    stages {
    
        stage('GIT') 
         { 
            git 'https://github.com/gkdevops/PetClinic'
         }
         
        stage('Build') {
            steps {
                 def myhome=tool name: 'Maven', type: 'maven'
                 sh "${myhome}/bin/mvn package"
            }
        }
    }
}
