pipeline {
    agent any

    stages {
    
        stage('GIT') 
         { 
          steps {
            git 'https://github.com/gkdevops/PetClinic'
            }
         }
         
        stage('Build') {
            steps {
            environment {
                    myhome=tool name: 'Maven', type: 'maven'
                  }
                 
                 sh '${myhome}/bin/mvn package'
            }
        }
    }
}
